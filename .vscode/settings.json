{
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "server": "localhost",
            "port": 5432,
            "driver": "PostgreSQL",
            "name": "Local DB",
            "database": "insights-dev",
            "username": "postgres"
        }
    ],
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
    },
    "cSpell.words": [
        "froyotech",
        "headshots",
        "reqwest",
        "ubers"
    ],
    "cSpell.enabled": false,
    "editor.formatOnSave": true,
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "[typescript]": {
        "editor.tabSize": 2
    },
    "[svelte]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.insertSpaces": true
    },
}